<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Enable App Sandbox for child processes -->
    <key>com.apple.security.app-sandbox</key>
    <true/>

    <!-- Inherit entitlements from parent -->
    <key>com.apple.security.inherit</key>
    <true/>

    <!-- JIT compilation for Electron child processes -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>

    <!-- Unsigned executable memory for Electron child processes -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>

    <!-- Disable library validation for Electron child processes -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
</dict>
</plist>
